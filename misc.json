{
	"request": [
		{
			"enable": true,
			"name": "Google Sorry",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://(ipv[46]|www)\\.google\\.com/sorry/index\\?continue=(.*)&q=(.*)",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "$2",
			"group": "Ungrouped"
		}
	],
	"sendHeader": [],
	"receiveHeader": []
}
